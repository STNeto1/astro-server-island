---
const timestamp = await new Promise<{ started: string; finished: string }>(
    async (res) => {
        const started = new Date().toISOString();
        await new Promise((resolve) => setTimeout(resolve, 1000));
        return res({
            started,
            finished: new Date().toISOString(),
        });
    },
);
---

<div>
    <p>Started {timestamp.started}</p>
    <p>Finished {timestamp.finished}</p>
</div>
